<Configurations>
	<!-- How tall the tree posts will be in meters --> 
	<Configure name="TreeHeight">2</Configure>

	<!-- The separation distance between each row -->
	<Configure name="RowWidth">20</Configure>

	<!-- The number of rows -->
	<Configure name="RowNumber">10</Configure>

	<!-- The length of each row -->
	<Configure name="ColumnLength">20</Configure>

	<!-- The separation between each post in the row -->
	<Configure name="PostSpacing">1</Configure>
</Configurations>


